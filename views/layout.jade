doctype 5
html
  head
    title Generated with Bumm
    meta(charset='UTF-8')
    link(rel='stylesheet', href='/vendor/bootstrap/css/bootstrap.min.css')
    link(rel='stylesheet', href='/css/custom.css')
    block scripts
      script(src='/vendor/jquery/js/jquery.min.js')
      script(src='/vendor/bootstrap/js/bootstrap.min.js')
  body
    div.navbar.navbar-inverse.navbar-static-top(role='navigation')
      .container
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/') JSBlog
        .collapse.navbar-collapse
          ul.nav.navbar-nav
            li
              a(href='/users')= __("Users")
            li
              a(href='/posts')= __("Posts")
            li
              a(href='/comments')= __("Comments")
            li
              a(href='/links')= __("Links")
            li
              a(href='/options')= __("Options")
            li
              a(href='/terms')= __("Terms")
            li
              a(href='/termtaxonomies')= __("Termtaxonomies")
            li
              a(href='/termrelationships')= __("Termrelationships")
          ul.nav.navbar-nav.navbar-right
            if session && session.user
              li
                a(href='/profile')= __("Profile")
              li
                a(href='/logout')= __("Logout")
            else
              li
                a(href='/login')= __("Login")
    .container
      block content